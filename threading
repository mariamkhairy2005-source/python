
import threading 
import time
def print_1():
     print("starting of thread:",threading.current_thread().name)
     print("finishing of thread:",threading.current_thread().name)

def print_2():
         print("start of thread:",threading.current_thread().name) 
         print("finish of thread:",threading.current_thread().name)
         
a=threading.Thread(target=print_1,name="thread_1") 
b=threading.Thread(target=print_2,name="thread_2") 
a.start() 
b.start()
# Project implementation complete
